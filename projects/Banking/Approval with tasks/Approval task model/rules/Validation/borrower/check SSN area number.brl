<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>check SSN area number</name>
  <uuid>1c8de1f0-382d-41c5-9e54-c75427dea46b</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[if 
    the area number of the SSN of 'the borrower' is one of { "000" , "666" } 
then 
	'the report' does not get data validated ;
    'the report' does not get approved ; 
    add "The borrower's SSN area number cannot be " + the area number of the SSN of 'the borrower' to the messages of 'the report' ; 
    print "rejected with The borrower's SSN area number cannot be " + the area number of the SSN of 'the borrower';
else 
    print "The borrower's SSN area number belongs to an authorized area."; 

]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
